DEF OPENTEXT Transform { translation  0 3 -5  children [
Shape {appearance Appearance {material Material {diffuseColor 0.0 1.0 1.0}}
        geometry Text {
                string ["Choose a new VRML/X3D" "file with the menu" "New button"]
		fontStyle FontStyle{
			justify ["MIDDLE","MIDDLE","MIDDLE"]
			size 0.5
		}
        }
}
]}


Transform {
        rotation 1 0 0 0.4
        translation 0 -2 3
        children [
                DEF BOX Transform { children Shape {
                        appearance Appearance {
                                material Material { }
				texture ImageTexture {url "startupImage.jpg"}
                        }
                        geometry Box { }
                }
        }

        ]
}


# Animation clock
        DEF Clock TimeSensor {
                cycleInterval 6.0
                loop TRUE
        },

# Animation path

        DEF ColumnPath OrientationInterpolator {
                key [ 0.0 0.5 1.0 ]
                keyValue [
                        0.0 1.0 0.0 6.28,
                        0.0 1.0 0.0 3.14,
                        0.0 1.0 0.0 0.0,
                ]
}

ROUTE Clock.fraction_changed TO ColumnPath.set_fraction
ROUTE ColumnPath.value_changed TO BOX.set_rotation
ROUTE ColumnPath.value_changed TO OPENTEXT.set_rotation

